{
	"name": "@abysslabs/cli",
	"version": "0.0.2",
	"author": "Akin (https://github.com/a-short-dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ajoralabs/abyss.git",
		"directory": "apps/cli"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"bin": {
		"abyss": "dist/index.js"
	},
	"scripts": {
		"dev": "bun run src/index.ts",
		"start": "node dist/index.js",
		"build": "bun run build:gui && cp -r ../gui/dist ./client && bun build ./src/index.ts --outdir ./dist --target node",
		"build:gui": "cd ../gui && bun run build"
	},
	"files": [
		"dist",
		"client",
		"README.md",
		"LICENSE"
	],
	"dependencies": {
		"boxen": "8.0.1",
		"chalk": "5.6.2",
		"citty": "^0.1.6",
		"gradient-string": "3.0.0",
		"h3": "2.0.1-rc.14",
		"listhen": "1.9.0",
		"mime-types": "3.0.2",
		"open": "^10.1.0",
		"ora": "9.3.0",
		"prompts": "2.4.2",
		"srvx": "0.11.5"
	},
	"devDependencies": {
		"@types/bun": "1.3.9",
		"@types/mime-types": "3.0.1",
		"@types/node": "^24.10.1",
		"@types/prompts": "2.4.9",
		"typescript": "~5.9.3"
	}
}
